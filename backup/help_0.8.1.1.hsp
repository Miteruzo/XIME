		Case $078
		Case $079
;			ErrorMes "此機能は現在實裝してをりません。","このき　のぅ　　げんざいじっさぅ
			SwBreak
		Case $0BE	; .
			If selecting@==1{
				selecting@=2
				selx@(1)=selx@
				sely@(1)=sely@
			}
			SwBreak
		Case $170	; SHIFT+f･1
			If menuing@==0{
				gsel2@^1
				gSel 0,gsel2@+1
			}
			SwBreak
		Case $174	; SHIFT+f･5
			input_text@+inpnot_text@
