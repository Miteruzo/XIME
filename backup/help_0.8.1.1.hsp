			SwBreak
		Case $071	; f･2
			ErrorMes "此機能は現在實裝してをりません。","このき　のぅ　　げんざいじっさぅ
/*			If topping==0{
				topping=1
				selecting@=1
				selx@=0
				sely@=0
				selx@(1)=0
				sely@(1)=0
			}*/
			SwBreak
		Case $072	; f･3
			If menuing@==0{
				square@^1
			}
			SwBreak
		Case $073	; f･4
			ErrorMes "此機能は現在實裝してをりません。","このき　のぅ　　げんざいじっさぅ
/*			If topping==0{
				topping=1
				searching@=1
				input_text@="
