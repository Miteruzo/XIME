	GoSub *SetPos
	NewTab "文章
	SysColor 4
	BoxF
	ObjSize 300,80
	GroupBox RepSimple("字體",kanzi,kana)
	Pos 0,88
	GroupBox RepSimple("假名遣",kanzi,kana)
	ObjSize 120,24
	Pos 30,22
